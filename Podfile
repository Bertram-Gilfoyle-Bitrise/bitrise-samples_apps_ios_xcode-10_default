# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Xcode-10_default' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Xcode-10_default
  pod 'SnapKit', '~> 4.2'

  target 'Xcode-10_defaultTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SnapKit', '~> 4.2'
  end

  target 'Xcode-10_defaultUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SnapKit', '~> 4.2'
  end

end
